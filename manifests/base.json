{
  "name": "NXDomain'd",
  "version": "1.0.7",
  "description": "Discover requests sent to unregistered domains.",
  "icons": {
    "16": "icons/icon16-blue.png",
    "32": "icons/icon32-blue.png",
    "128": "icons/icon128-blue.png"
  },
  "permissions": ["storage", "tabs", "webNavigation", "alarms"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["csp-listener.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "NXDOMAIN'd"
  },
  "homepage_url": "https://github.com/megamansec/nxdomaind"
}
